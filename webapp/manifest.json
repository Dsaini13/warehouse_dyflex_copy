{
	"_version": "1.12.0",
	"sap.app": {
		"id": "dyflex.mm.s4cloud.warehouse",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"resources": "resources.json",
		"dataSources": {
			"MatlStock": {
				"uri": "/S4HANA_CLOUD/sap/opu/odata/sap/API_MATERIAL_STOCK_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"PurchOrder": {
				"uri": "/S4HANA_CLOUD/sap/opu/odata/sap/API_PURCHASEORDER_PROCESS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/API_PURCHASEORDER_PROCESS_SRV/metadata.xml"
				}
			},
			"CustomStock": {
				"uri": "/S4HANA_CLOUD/sap/opu/odata/sap/YY1_WAREHOUSE_STOCK_CDS/",
				"type": "OData",
				"settings": {
					"localUri": "localService/YY1_WAREHOUSE_STOCK_CDS/metadata.xml"
				}
			},
			"Product": {
				"uri": "/S4HANA_CLOUD/sap/opu/odata/sap/API_PRODUCT_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/API_PRODUCT_SRV/metadata.xml"
				}
			},
			"PhysInvDoc": {
				"uri": "/S4HANA_CLOUD/sap/opu/odata/sap/API_PHYSICAL_INVENTORY_DOC_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/API_PHYSICAL_INVENTORY_DOC_SRV/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "dyflex.mm.s4cloud.warehouse.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.66.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "dyflex.mm.s4cloud.warehouse.i18n.i18n"
				}
			},
			"matlStockSrv": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "MatlStock",
				"preload": true
			},
			"purchOrderSrv": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "PurchOrder",
				"preload": true
			},
			"customStockSrv": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "CustomStock",
				"preload": true
			},
			"productSrv": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Product",
				"preload": true
			},
			"physInvDocSrv": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "PhysInvDoc",
				"preload": true
			}
		},
		"services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService",
				"lazy": false,
				"settings": {
					"setTitle": "auto"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "dyflex.mm.s4cloud.warehouse.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "mainList",
					"target": [
						"mainList"
					]
				},
				{
					"pattern": "StockOverview",
					"name": "stockOverviewList",
					"target": [
						"stockOverviewList"
					]
				},
				{
					"pattern": "StockOverviewDetail/{objectId}",
					"name": "stockOverviewDetail",
					"target": [
						"stockOverviewDetail"
					]
				},
				{
					"pattern": "Stocktake",
					"name": "stocktakeList",
					"target": [
						"stocktakeList"
					]
				},
				{
					"pattern": "GoodsReceipt",
					"name": "goodsReceiptList",
					"target": [
						"goodsReceiptList"
					]
				}
			],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1,
					"title": "{i18n>worklistViewTitle}"
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2,
					"title": "{i18n>objectViewTitle}"
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"mainList": {
					"viewType": "XML",
					"viewName": "MainList"
				},
				"stockOverviewList": {
					"viewType": "XML",
					"viewName": "StockOverviewList"
				},
				"stockOverviewDetail": {
					"viewType": "XML",
					"viewName": "StockOverviewDetail"
				},
				"stocktakeList": {
					"viewType": "XML",
					"viewName": "StocktakeList"
				},
				"goodsReceiptList": {
					"viewType": "XML",
					"viewName": "GoodsReceiptList"
				}
			}
		}
	}
}