<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:upload="sap.m.upload"
	xmlns:core="sap.ui.core">
	
	<upload:UploadSet
		id="idUploadCollection"
		uploadEnabled="true"
		instantUpload="false"
		maxFileSize="10"
		beforeUploadStarts="onBeforeUploadStarts"
		fileSizeExceeded="onFileSizeExceeded">
		<upload:toolbar>
			<OverflowToolbar>
				<Title text="Attachments" />
				<ToolbarSpacer/>
			</OverflowToolbar>
		</upload:toolbar>
	</upload:UploadSet>
	
</core:FragmentDefinition>